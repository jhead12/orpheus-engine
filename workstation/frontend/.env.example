# Environment Configuration
NODE_ENV=development

# Server Configuration
VITE_PORT=5174
VITE_HOST=true
VITE_API_PROTOCOL=http
VITE_API_HOST=localhost
VITE_API_PORT=5001

# Audio Processing Configuration
VITE_AUDIO_SAMPLE_RATE=44100
VITE_AUDIO_BUFFER_SIZE=4096
VITE_AUDIO_LATENCY_HINT=interactive
VITE_AUDIO_PROCESSING_PROTOCOL=http
VITE_AUDIO_PROCESSING_HOST=localhost
VITE_AUDIO_PROCESSING_PORT=7008

# Monitoring Configuration
VITE_MONITOR_PROTOCOL=http
VITE_MONITOR_HOST=localhost
VITE_MONITOR_PORT=8000

# Feature Flags
VITE_ENABLE_AUDIO_WORKLET=true
VITE_ENABLE_WASM=true

# Development Tools
VITE_ENABLE_DEVTOOLS=true
