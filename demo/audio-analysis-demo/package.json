{
  "name": "orpheus-audio-analysis-demo",
  "version": "1.0.0",
  "description": "Audio Recording and AI Analysis Demo for HP AI Studio Competition",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
